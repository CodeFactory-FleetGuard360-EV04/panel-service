# Server Configuration
SERVER_PORT=8080

# Database Configuration - PostgreSQL
DB_URL=jdbc:postgresql://localhost:5432/panel_db
DB_USERNAME=
DB_PASSWORD=

# JPA Configuration
JPA_DDL_AUTO=update
JPA_SHOW_SQL=true

# RabbitMQ Configuration
RABBITMQ_HOST=
RABBITMQ_PORT=5672
RABBITMQ_USERNAME=
RABBITMQ_PASSWORD=

# RabbitMQ Queue Configuration
RABBITMQ_QUEUE_NAME=alerts.queue
RABBITMQ_EXCHANGE_NAME=alerts.exchange
RABBITMQ_ROUTING_KEY=alerts.routing.key

# GraphQL Configuration
GRAPHQL_GRAPHIQL_ENABLED=true

# Logging Configuration
LOG_LEVEL=INFO
APP_LOG_LEVEL=DEBUG

